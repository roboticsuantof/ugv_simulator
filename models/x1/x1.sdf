      <plugin name="gps_controller_siar" filename="libplugin_gazebo_ros_gps.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>10.0</updateRate>
        <bodyName>base_link</bodyName>
        <robotBaseFrame>base_link</robotBaseFrame>
        <!-- <frameId>siar/base_link</frameId> -->
        <topicName>gps</topicName>
        <velocityTopicName>gps_velocity</velocityTopicName>
        <drift>5.0 5.0 5.0</drift>
        <gaussianNoise>0.1 0.1 0.1</gaussianNoise>
        <velocityDrift>0 0 0</velocityDrift>
        <velocityGaussianNoise>0.1 0.1 0.1</velocityGaussianNoise>
        <referenceLatitude>49.9</referenceLatitude>
        <referenceLongitude>8.9</referenceLongitude>
        <referenceHeading>0</referenceHeading>
        <referenceAltitude>0</referenceAltitude>
      </plugin>
    
      <plugin name="diffdrive_plugin_multi_wheel" filename="libplugin_diffdrive_plugin_multi_wheel.so">
    
        <!-- Plugin update rate in Hz -->
        <updateRate>500</updateRate>
        <!-- Name of left joint, defaults to `left_joint` -->
        <!--leftJoint>base_link_left_wheel_joint</leftJoint-->
        <leftJoints>rear_left_wheel_joint front_left_wheel_joint</leftJoints>
        <!-- Name of right joint, defaults to `right_joint` -->
        <!--rightJoint>base_link_right_wheel_joint</rightJoint-->
        <rightJoints>rear_right_wheel_joint front_right_wheel_joint</rightJoints>
        <!-- The distance from the center of one wheel to the other, in meters, defaults to 0.34 m -->
        <wheelSeparation>0.6380</wheelSeparation>
